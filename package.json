{
  "name": "clode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "build:start": "next build && next start",
    "lint": "next lint",
    "database:push": "prisma db push && prisma generate",
    "database:studio": "prisma studio --port 8888",
    "format:write": "prettier --write app/* core/**/*",
    "format:check": "prettier --check app/* core/**/*",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "environment:push": "npx dotenv-vault@latest push && npx dotenv-vault@latest push production",
    "environment:push:development": "npx dotenv-vault@latest push",
    "environment:push:production": "npx dotenv-vault@latest push production",
    "environment:pull": "npx dotenv-vault@latest pull && npx dotenv-vault@latest pull production",
    "environment:pull:development": "npx dotenv-vault@latest pull",
    "environment:pull:production": "npx dotenv-vault@latest pull production",
    "prepare": "husky",
    "commit": "next lint && prettier --check app/* core/**/* && vitest run"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.3.3",
    "@heroicons/react": "^2.1.1",
    "@nextui-org/react": "^2.2.9",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^5.28.6",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.4",
    "framer-motion": "^11.0.3",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.9",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "resend": "^3.1.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.2.2"
  }
}
